<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.0</TargetFramework>
    <Authors>Allan Lindqvist</Authors>
    <Company />
    <Description>Library for data binding to IObservables in xaml</Description>
    <PackageProjectUrl>https://github.com/aL3891/ObservableDatabinding</PackageProjectUrl>
    <PackageTags>xaml databinding rx IObservable</PackageTags>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Reactive.Core" Version="3.1.1" />
    <PackageReference Include="System.Reactive.Linq" Version="3.1.1" />
  </ItemGroup>

</Project>